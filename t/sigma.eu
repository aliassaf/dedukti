o : Type.
eps : o -> Type.

sigma_ : A : o -> P : (x : eps A -> o) -> o.
exist_ : A : o -> P : (x : eps A -> o) -> x : eps A -> eps (P x) -> eps (sigma_ A P).

fst : A : o -> P : (x : eps A -> o) -> eps (sigma_ A P) -> eps A.
[A : o, P : x : eps A -> o, w : eps A, pi : P w]
fst _ _ (eps (exist_ _ _ w pi)) --> w.

snd : A : o -> P : (x : eps A -> o) -> s : eps (sigma_ A P) -> eps (P (fst A P s)).
[A : o, P : x : eps A -> o, w : eps A, pi : P w]
snd _ _ (eps (exist_ _ _ w pi)) --> pi.
